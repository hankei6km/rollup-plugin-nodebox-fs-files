    "clean": "rimraf \"dist/*\"",
    "upgrade-interactive": "npm-check --update",
    "csb:test": "npm test -- --runInBand --watchAll"
  },
  "devDependencies": {
    "@swc/core": "^1.3.58",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.5",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
